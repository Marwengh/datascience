    "from textblob import TextBlob\n",
    "example=\"You're not good but you're beautiful\"\n",
    "analysis = TextBlob((example))\n",
    "        # set sentiment\n",
    "if analysis.sentiment.polarity > 0:\n",
    "    print ('positive')\n",
    "elif analysis.sentiment.polarity == 0:\n",
    "    print ('neutral')\n",
    "else:\n",
    "   print ('negativ')"
